
<h2>See all my cards so far!</h2>
<ul id="allCards">
    {{#each allCards}}
    <li>
        <div class="allCardsCard">
            <img class="randomCard" src="{{Images.0.src}}" alt="Card {{cardNumber}}: {{cardName}}">
            <h3>{{cardName}}</h3>
        </div>
    </li>
    {{/each}}
</ul>
<script src="/js/evenOdd.js"></script>